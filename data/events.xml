<events>
  {% for events in site.data.events %}
  <events>
    <title>{{ events[0] }}</title>
    <date>{{ events[1].date }}</date>
    <url>{{ events[1].url }}</url>
    <location>{{ events[1].location }}</location>
    <description>{{ events[1].description }}</description>
  </events>
  {% endfor %}
</events>
